{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "irecommendthis/recommend",
    "version": "4.0.0",
    "title": "I Recommend This",
    "category": "widgets",
    "icon": "thumbs-up",
    "description": "Block to add recommend link.",
    "supports": {
        "html": false
    },
    "attributes": {
        "postId": {
            "type": "number",
            "default": null
        },
        "alignText": {
            "type": "string",
            "default": "left"
        },
        "useCurrentPost": {
            "type": "boolean",
            "default": true
        }
    },
    "textdomain": "i-recommend-this",
    "editorScript": "file:./index.js"
}
